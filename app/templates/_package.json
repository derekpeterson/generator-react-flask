{
  "name": "<%= kebabName %>",
  "version": "1.0.0",
  "description": "Front-end for <%= kebabName %>",
  "main": "src/js/main.js",
  "scripts": {
    "app": "npm run prod && python <%= snakeName %>.py",
    "compile": "gulp compile",
    "lint": "eslint src/js",
    "prod": "gulp prod",
    "test": "mocha --compilers js:babel-register --recursive test/*",
    "watch": "gulp watch"
  },
  "author": "<%= user.name %> <<%= user.email %>>",
