  },
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.3.0",
    "eslint": "^1.6.0",
    "gulp": "^3.9.0",
    "gulp-less": "^3.0.3",
    "gulp-clean-css": "^2.0.7",
    "gulp-watch": "^4.3.5",
    "less-plugin-autoprefix": "^1.5.1",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.0",
    "sinon": "^1.17.1"
  },
  "dependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "normalize.less": "^1.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "webpack": "^1.13.1"
  }
